{
  "name": "hellrazor",
  "version": "0.1.5",
  "description": "Create custom errors for your JavaScript projects",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run splice && npm run uglify",
    "clean": "rimraf *.min.* hellrazor.js",
    "browser": "cpy index.js --rename=hellrazor.js ./",
    "splice": "head -n -9 index.js > hellrazor.js",
    "uglify": "npm run uglify-npm && npm run uglify-browser",
    "uglify-npm": "uglifyjs index.js --output index.min.js --source-map index.min.js.map",
    "uglify-browser": "uglifyjs hellrazor.js --output hellrazor.min.js --source-map hellrazor.min.js.map",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "JavaScript",
    "javascript",
    "js",
    "node.js",
    "custom error",
    "custom errors",
    "hellrazor",
    "Buglife",
    "Bug Life"
  ],
  "author": "Khaled Al-Ansari <khaledelansari@gmail.com> (http://khaledelansari.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhaledElAnsari/Hellrazor.js.git"
  },
  "bugs": {
    "url": "https://github.com/KhaledElAnsari/Hellrazor.js/issues"
  },
  "homepage": "https://github.com/KhaledElAnsari/Hellrazor.js#readme",
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "rimraf": "^2.5.4",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {}
}
